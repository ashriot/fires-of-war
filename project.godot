; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Fires of War"
run/main_scene="res://src/scenes/map.tscn"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://icon.svg"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=110
window/size/viewport_height=190
window/size/window_width_override=660
window/size/window_height_override=1140
window/stretch/mode="canvas_items"

[gui]

theme/custom="res://default_theme.tres"
theme/custom_font="res://assets/fonts/TinyBox.ttf"
theme/default_font_antialiasing=0
theme/default_font_hinting=0
theme/default_font_subpixel_positioning=0

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
